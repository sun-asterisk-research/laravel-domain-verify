{
    "name": "sun-asterisk/laravel-domain-verify",
    "description": "Verify domain ownership for Laravel application",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.1.3",
        "illuminate/database": "~5.6.0|~5.7.0|~5.8.0|^6.0",
        "illuminate/hashing": "^5.0",
        "illuminate/support": "~5.6.0|~5.7.0|~5.8.0|^6.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.2",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "SunAsterisk\\DomainVerifier\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SunAsterisk\\DomainVerifier\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
